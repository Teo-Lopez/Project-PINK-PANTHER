<div class="container">
        
    <div class="input-group rounded">
        <input type="search" class="form-control rounded" placeholder="Search" aria-label="Search"
            aria-describedby="search-addon" />
        <span class="input-group-text border-0" id="search-addon">
            <i class="fas fa-search"></i>
        </span>
    </div>
    
</div>

<div class="container">
    <h2>Vista de tendencias</h2>
    {{#each allUploads}}
    <div class="row">

      <a href="tendencias/detalles/{{this.0.tagId.id}}"><p>{{this.0.tagId.name}}</p></a>  
        
        <div class="col">

        {{#each this}}

            <a href="tendencias/detalles/{{../this.0.tagId.id}}"><img src="{{img}}" alt="imagen"></a>
    
        {{/each}}
        
        </div>
    </div>
    {{/each}} 
</div>


