<div class="container">
<h2>random</h2>
    
</div>
<div class="container">
    <div class="row d-flex justify-content-center">
            <label for="tagId"></label>
            <input class="btn btn-outline-success btn-block" pattern="#" name="tagId" id="tagId" placeholder="search for the white rabbit" >
        <div class="col-11 column" id="feed">
            {{#each result}}
            <h4>{{tagId.name}}</h4>
            <img class="resolve" src="{{img}}" alt="imagen">
            {{#if ../isUser}} 
            <a href='/fotograma/detalles/{{id}}' class="btn btn-outline-success"> details</a>
            {{/if}}
            <hr>
            {{/each}}
        </div>
    </div>
</div>
<script src="js/searchbar.js"></script>